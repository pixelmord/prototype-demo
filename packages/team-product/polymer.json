{
  "shell": "src/start-lit-element.js",
  "entrypoint": "static/index.html",
  "fragments": ["src/lazy-element.js"],
  "npm": true,
  "moduleResolution": "node",
  "sources": ["src/start-lit-element.js", "package.json"],
  "extraDependencies": ["node_modules/@webcomponents/webcomponentsjs/**"],
  "builds": [
    {
      "bundle": true,
      "js": {
        "minify": false,
        "compile": "es5",
        "transformModulesToAmd": true
      },
      "addServiceWorker": false,
      "addPushManifest": false
    }
  ]
}
